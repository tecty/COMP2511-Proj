<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <VBox alignment="CENTER" layoutX="50.0" layoutY="-8.0" prefHeight="300.0" prefWidth="200.0" spacing="12.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Gridlock">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <Button fx:id="newGameButton" defaultButton="true" mnemonicParsing="false" onAction="#modeSelectAction" prefWidth="135.0" text="New game" />
            <Button fx:id="continueButton" mnemonicParsing="false" onAction="#levelSelectAction" prefWidth="135.0" text="Continue" />
            <Button fx:id="settingButton" mnemonicParsing="false" onAction="#settingAction" prefWidth="135.0" text="Settings" />
            <Button fx:id="quitButton" mnemonicParsing="false" onAction="#quitAction" prefWidth="135.0" text="Quit" />
         </children>
      </VBox>
   </children>
</AnchorPane>
