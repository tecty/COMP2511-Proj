<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.GameController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <Pane fx:id="rootPane" prefHeight="300.0" prefWidth="300.0" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="50.0" left="60.0" top="20.0" />
               </BorderPane.margin></Pane>
         </center>
         <top>
            <Label text="Gridlock Leve 1" BorderPane.alignment="CENTER">
               <font>
                  <Font size="43.0" />
               </font>
            </Label>
         </top>
         <right>
            <Pane prefHeight="200.0" prefWidth="150.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox alignment="CENTER" prefHeight="349.0" prefWidth="150.0">
                     <children>
                        <Button defaultButton="true" mnemonicParsing="false" prefWidth="100.0" text="Hint">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin></Button>
                        <Button mnemonicParsing="false" prefWidth="100.0" text="Reset">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin></Button>
                        <Button cancelButton="true" mnemonicParsing="false" prefWidth="100.0" text="Skip">
                           <VBox.margin>
                              <Insets bottom="20.0" />
                           </VBox.margin></Button>
                        <GridPane>
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="83.0" minWidth="10.0" prefWidth="83.0" />
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="70.0" minWidth="10.0" prefWidth="67.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <Label text="Total Steps:" />
                              <Label text="Total Time:" GridPane.rowIndex="1" />
                              <GridPane GridPane.columnIndex="1">
                                <columnConstraints>
                                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="28.0" minWidth="10.0" prefWidth="28.0" />
                                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="42.0" minWidth="10.0" prefWidth="39.0" />
                                </columnConstraints>
                                <rowConstraints>
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                </rowConstraints>
                                 <children>
                                    <Label fx:id="stepCount" text="0" GridPane.halignment="CENTER" />
                                    <Label text="steps" GridPane.columnIndex="1" />
                                 </children>
                              </GridPane>
                              <GridPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                                 <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                 </columnConstraints>
                                 <rowConstraints>
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                 </rowConstraints>
                                 <children>
                                    <Label fx:id="timeCount" text="0" GridPane.halignment="CENTER" />
                                    <Label text="secs" GridPane.columnIndex="1" />
                                 </children>
                              </GridPane>
                           </children>
                        </GridPane>
                     </children>
                  </VBox>
               </children>
               <BorderPane.margin>
                  <Insets right="50.0" />
               </BorderPane.margin></Pane>
         </right>
      </BorderPane>
   </children>
</AnchorPane>
